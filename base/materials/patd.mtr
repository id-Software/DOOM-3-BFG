	{
		if ( ( time * 6 ) % 6 == 2 )
		blend blend
		map		guis/assets/small/sm3num3.tga
	}	
	{
		if ( ( time * 6 ) % 6 == 3 )
		blend blend
		map		guis/assets/small/sm3num4.tga
	}	
	{
		if ( ( time * 6 ) % 6 == 4 )
		blend blend
		map		guis/assets/small/sm3num5.tga
	}	
	{
		if ( ( time * 6 ) % 6 == 5 )
		blend blend
		map		guis/assets/small/sm3num6.tga
	}
}

